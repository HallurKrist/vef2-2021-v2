
<table>
  <tr>
    <th class='list__head'>Dags</th>
    <th class='list__head'>Nafn</th>
    <th class='list__head'>Athugasemd</th>
  </tr>
  <% for(var i = 0; i < signatures.length; i++) { %>
    <tr>
      <td class='list__day'><%- signatures[i].signed.toLocaleDateString().replace('/','.').replace('/','.') %></th>
      <td class='list__name'><%- signatures[i].name %></th>
      <td class='list__comment'><%- signatures[i].comment %></th>
    </tr>
  <% } %>
</table>



